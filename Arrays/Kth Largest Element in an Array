import java.util.*;
public class java{
    public static void main(String[] args) {
        int n,i,j;
        int uniqueCount = 0;
        boolean flag;
        Scanner sc=new Scanner(System.in);
        n=sc.nextInt();
        int arr[]=new int[n];
        int unique[]=new int[n];
        for(i=0;i<n;i++){
            arr[i]=sc.nextInt();
        }
        Arrays.sort(arr);
        int k=sc.nextInt();
        System.out.println(arr[n-k]);
    }
}
